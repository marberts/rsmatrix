> library(rsmatrix)
> 
> #---- Tests for finding previous values ----
> prev2 <- function(x) {
+   dr <- match(x, sort(unique(x)))
+   match(pmax(dr - 1L, 1L), dr, incomparables = NA)
+ }
> 
> set.seed(1432)
> x <- sample(1e3)
> y <- replace(x, sample(1e3, 10), NA)
> 
> all.equal(rsmatrix:::prev(x), prev2(x))
[1] TRUE
> all.equal(rsmatrix:::prev(y), prev2(y))
[1] TRUE
